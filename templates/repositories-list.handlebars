{{#each elements}}
  <a id="{{name}}" class="select-menu-item js-navigation-item js-navigation-open gh-projects-options-repositories-toggle {{#if isSelected}}selected{{/if}}">
    <svg aria-hidden="true" class="octicon octicon-check select-menu-item-icon" height="16" version="1.1" viewBox="0 0 12 16" width="12">
      <path fill-rule="evenodd" d="M12 5l-8 8-4-4 1.5-1.5L4 10l6.5-6.5z"></path>
    </svg>
    <div class="color-label-wrapper css-truncate-target">
      <span class="color" style="{{prop}}">&nbsp;</span>
      <span class="name">{{name}}</span>
    </div>
  </a>
{{/each}}
